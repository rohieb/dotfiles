set mainfont {{DejaVu Sans} 9}
set textfont {{DejaVu Sans Mono} 9}
set uifont {{DejaVu Sans} 9}
set tabstop 4
set findmergefiles 0
set maxgraphpct 50
set maxwidth 16
set cmitmode tree
set wrapcomment none
set autoselect 1
set autosellen 40
set showneartags 1
set hideremotes 1
set showlocalchanges 1
set datetimeformat {%Y-%m-%d %H:%M:%S}
set limitdiffs 1
set uicolor grey85
set want_ttk 0
set bgcolor white
set fgcolor black
set colors {green red blue magenta darkgrey brown orange}
set diffcolors {red "#00a000" blue}
set markbgcolor #e0e0ff
set diffcontext 3
set selectbgcolor gray85
set extdifftool meld
set perfile_attrs 0
set geometry(main) 1364x718+0+19
set geometry(state) normal
set geometry(topwidth) 1362
set geometry(topheight) 232
set geometry(pwsash0) "628 1"
set geometry(pwsash1) "891 1"
set geometry(botwidth) 668
set geometry(botheight) 474
set permviews {{{All branches} {} {--all --branches --tags --remotes} {}}
}
