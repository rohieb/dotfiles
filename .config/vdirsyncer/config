[general]
status_path = "~/.config/vdirsyncer/status/"


[pair neptun_home]
a = "neptun_home_remote"
b = "neptun_home_local"
collections = null
conflict_resolution = ["command", "meld" ]

[storage neptun_home_remote]
type = "caldav"
url = "https://rohieb.name:5233/calendar/rohieb/home"
username = "rohieb"
password.fetch = ["command", "~/.config/vdirsyncer/get-password.sh", "neptun"]
item_types = ["VEVENT", "VCONTACT"]

[storage neptun_home_local]
type = "filesystem"
path = "~/.local/share/vdirsyncer/neptun_home"
fileext = ".ics"


